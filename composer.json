{
    "name": "willitscale/streetlamp",
    "description": "A HTTP router application wrapper.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "willitscale\\Streetlamp\\": "src/",
            "willitscale\\StreetlampTest\\": "src-test/",
            "willitscale\\StreetlampTests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "willitscale"
        }
    ],
    "require": {
        "psr/log": "^3.0",
        "psr/http-message": "^2.0",
        "psr/http-server-middleware": "^1.0",
        "php-di/php-di": "^7.0"
    },
    "config": {
        "platform": {
            "php": "8.4.0"
        }
    },
    "bin": [
        "streetlamp"
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.2",
        "squizlabs/php_codesniffer": "^3.13"
    }
}
